FUNCTION squaRever :
PARAM num
flag := #0
DEC array 12
j := #0
TEMPVAR4 := &array + #0
TEMPVAR7 := num / #100
*TEMPVAR4 := TEMPVAR7
TEMPVAR11 := &array + #4
TEMPVAR15 := num / #10
TEMPVAR20 := &array + #0
TEMPVAR16 := #10 * *TEMPVAR20
TEMPVAR14 := TEMPVAR15 - TEMPVAR16
*TEMPVAR11 := TEMPVAR14
TEMPVAR24 := &array + #8
TEMPVAR33 := &array + #0
TEMPVAR31 := #100 * *TEMPVAR33
TEMPVAR28 := num - TEMPVAR31
TEMPVAR37 := &array + #4
TEMPVAR29 := #10 * *TEMPVAR37
TEMPVAR27 := TEMPVAR28 - TEMPVAR29
*TEMPVAR24 := TEMPVAR27
TEMPVAR40 := &array + #0
TEMPVAR41 := &array + #8
IF *TEMPVAR40 == *TEMPVAR41 GOTO LABEL2
flag := #0
GOTO LABEL6
LABEL LABEL2 :
IF j >= #12 GOTO LABEL6
TEMPVAR53 := j * j
TEMPVAR51 := TEMPVAR53 - num
IF TEMPVAR51 != #0 GOTO LABEL8
flag := #1
LABEL LABEL8 :
TEMPVAR62 := j + #1
j := TEMPVAR62
GOTO LABEL2
LABEL LABEL6 :
IF flag != #1 GOTO LABEL10
RETURN #1
GOTO LABEL11
LABEL LABEL10 :
RETURN #0
LABEL LABEL11 :
FUNCTION main :
i := #100
LABEL LABEL12 :
IF i >= #150 GOTO LABEL14
ARG i
TEMPVAR72 := CALL squaRever
IF TEMPVAR72 != #1 GOTO LABEL16
WRITE i
TEMPVAR75 := #0
LABEL LABEL16 :
TEMPVAR79 := i + #1
i := TEMPVAR79
GOTO LABEL12
LABEL LABEL14 :
RETURN #0
